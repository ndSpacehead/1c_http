			ЧастиПоля = СтрРазделить(Поле, "=");
			Ключ = ЧастиПоля[0];
			
			ЧастиПоля.Удалить(0);
			
			Значение = ?(ЧастиПоля.Количество() = 0, Неопределено, СтрСоединить(ЧастиПоля, "="));
		КонецЕсли;
		
